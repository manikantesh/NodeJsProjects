<h1>Order Details</h1>

<table border="1">
    <tr>
        <th></th>
        <th>Name</th>
        <th>Email</th>
        <th>ProductName</th>
        <th>Address</th>
        <th>Quantity</th>
        <th>Order_Date</th>
        <th>Order_Status</th>
    </tr>
    <% for(var i=0;i<data.length;i++) {%>
        <tr>
            <td><a href="/order/sendMail/<%=data[i].email%>/<%=data[i].orderstatus%>">Send Email</a></td>
            <td><%=data[i].name%></td>
            <td><%=data[i].email%></td>
            <td><%=data[i].productName%></td>
            <td><%=data[i].address%></td>
            <td><%=data[i].quantity%></td>
            <td><%=data[i].createdDate%></td>
            <td><%=data[i].orderstatus%></td>
        </tr>
    <%}%>
    <tr>

    </tr>
</table>